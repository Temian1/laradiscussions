{
  "name": "foundationapp/discussions",
  "description": "Discussions is a Simple Laravel Discussions Package",
  "autoload": {
    "psr-4": {
      "Foundationapp\\Discussions\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Foundationapp\\Discussions\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Bobby Iliev",
      "email": "bobby@bobbyiliev.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Foundationapp\\Discussions\\DiscussionsServiceProvider"
      ]
    }
  },
  "require-dev": {
    "pestphp/pest-plugin-livewire": "^2.0",
    "pestphp/pest-plugin-laravel": "^2.0",
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^8.3"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "require": {
    "livewire/livewire": "^2.12",
    "filament/notifications": "^2.0"
  }
}
